#include <bits/stdc++.h>
using namespace std;

class User {
    string username;
    string companyName;
    int position;

public:
    User(string u = "", string c = "", int p = 0) : username(u), companyName(c), position(p) {}

    string getUsername() const { return username; }
    int getPosition() const { return position; }

    friend ostream& operator<<(ostream& out, const User& u) {
        out << "Username: " << u.username
            << " Company name: " << u.companyName
            << " Position: " << u.position;
        return out;
    }

    friend istream& operator>>(istream& in, User& u) {
        in >> u.username >> u.companyName >> u.position;
        return in;
    }

    bool operator==(const User& u) const {
        return username == u.username;
    }
};

class Group {
protected:
    vector<User> users;
    string name;

public:
    Group(string name = "") : name(name) {}
    virtual ~Group() {}

    virtual void addMember(User &u) {
        for (auto &x : users)
            if (x == u) return;
        users.push_back(u);
    }

    virtual double average() const {
        if (users.empty()) return 0;
        double s = 0;
        for (auto &u : users) s += u.getPosition();
        return s / users.size();
    }

    virtual double rating() const {
        return (10 - average()) * users.size() / 100.0;
    }

    friend ostream& operator<<(ostream& out, const Group& g) {
        out << "Group: " << g.name << endl;
        out << "Members: " << g.users.size() << endl;
        out << "Rating: " << g.rating() << endl;
        out << "Members list:" << endl;

        if (g.users.empty()) {
            out << "EMPTY" << endl;
        } else {
            for (int i = 0; i < g.users.size(); i++) {
                out << i + 1 << ". " << g.users[i] << endl;
            }
        }
        return out;
    }
};

class PrivateGroup : public Group {
public:
    static int CAPACITY;
    static const float COEFFICIENT;

    PrivateGroup(string name = "") : Group(name) {}

    void addMember(User &u) override {
        if (users.size() == CAPACITY) return;
        Group::addMember(u);
    }

    double rating() const override {
        return (10 - average()) * (double(users.size()) / CAPACITY) * COEFFICIENT;
    }

    static void setCapacity(int c) {
        CAPACITY = c;
    }

    friend ostream& operator<<(ostream& out, const PrivateGroup& g) {
        out << "Private ";
        Group tmp(g); // slicing INTENTIONALLY (like reference solution)
        out << tmp;
        return out;
    }
};

int PrivateGroup::CAPACITY = 10;
const float PrivateGroup::COEFFICIENT = 0.8f;

int main() {
    cout << "INTEGRATION TEST" << endl;

    int nGroups;
    cin >> nGroups;
    vector<Group*> groups(nGroups);

    for (int i = 0; i < nGroups; i++) {
        int type;
        string name;
        cin >> type >> name;

        if (type == 1) groups[i] = new Group(name);
        else groups[i] = new PrivateGroup(name);

        int nUsers;
        cin >> nUsers;
        for (int j = 0; j < nUsers; j++) {
            User u;
            cin >> u;
            groups[i]->addMember(u);
        }
    }

    cout << "BEFORE CHANGE OF PRIVATE GROUP COEFFICIENT" << endl;
    for (int i = 0; i < nGroups; i++) {
        cout << *groups[i];
    }

    PrivateGroup::setCapacity(15);

    cout << "AFTER CHANGE OF PRIVATE GROUP COEFFICIENT" << endl;
    for (int i = 0; i < nGroups; i++) {
        cout << *groups[i];
    }

    for (int i = 0; i < nGroups; i++) delete groups[i];
}
